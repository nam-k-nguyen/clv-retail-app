<h2>Add Transaction</h2>
<form method="POST" action="/add/transaction">
    <input name="hshd_num" placeholder="HSHD_NUM" required />
    <input name="basket_num" placeholder="BASKET_NUM" required />
    <input name="purchase_date" type="date" required />
    <input name="product_num" placeholder="PRODUCT_NUM" required />
    <input name="spend" placeholder="SPEND" required />
    <input name="units" placeholder="UNITS" required />
    <input name="store_region" placeholder="STORE_REGION" required />
    <input name="week_num" placeholder="WEEK_NUM" required />
    <input name="year" placeholder="YEAR" required />
    <button type="submit">Add Transaction</button>
</form>

<hr />

